<%= render "shared/navbar" %>
<h1 class="titulo">Agentes</h1>
<% flash.each do |name, msg| %>
    <%= content_tag :div, msg, class: "alert-info" %>
<% end %>
<div class="container">
  <div class="row">
    <div class="column">
      <table class="agent_table_col table_standard ">
        <tbody>
          <% @users.each do |user| %>
            <tr>
                <%= render partial: "agent", object: user %>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
<td><%= link_to 'Principal', main_admin_path %></td>

